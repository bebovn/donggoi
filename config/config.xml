<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>Phần mềm dịch vụ Mai Linh</Name>
    <Version>1.0.0</Version>
    <Title>Phần mềm dịch vụ Isuzu Mai Linh</Title>
    <Publisher>Bao khoa soft</Publisher>
    <!-- Directory name is used in component.xml -->
    <StartMenuDir>MaiLinhDichVu</StartMenuDir>
    <TargetDir>@ThuMucCaiDat@</TargetDir>
    <CreateLocalRepository>true</CreateLocalRepository>
    <RemoteRepositories>
        <Repository displayname="Res dich vu Mai Linh">
            <Url>http://isuzuml.ddns.net/rep/dichvu</Url>
        </Repository>
    </RemoteRepositories>
</Installer>
